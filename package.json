    "lint": "gulp lint",
    "sauce": "BABEL_ENV=test karma start test/config/karma.sauce.conf.js",
    "start": "gulp server",
    "tdd": "BABEL_ENV=test karma start test/config/karma.conf.js --no-single-run --auto-watch",
