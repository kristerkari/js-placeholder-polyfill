    "sauce": "./node_modules/karma/bin/karma start test/config/karma.sauce.conf.js",
    "start": "./node_modules/.bin/gulp server",
    "tdd": "./node_modules/karma/bin/karma start test/config/karma.conf.js --no-single-run --auto-watch",
