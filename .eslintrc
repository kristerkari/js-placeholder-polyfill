{
    "env": {
      "browser": true,
      "jquery": true,
      "node": true,
      "amd": true,
      "es6": true,
      "jasmine": true
    },
    "ecmaFeatures": {
      "modules": true
    },
    "globals": {
      "YUI": false,
      "Y": false,
      "Form": false,
      "$F": true,
      "helpers": true,
      "placekeeper": true,
      "triggerEvent": false
    },
    "rules": {
        "semi": [2, "never"],
        "no-unexpected-multiline": 2,
        "no-empty-pattern": 2,
        "no-native-reassign": 2,
        "no-lonely-if": 2,
        "no-arrow-condition": 2,
        "no-inner-declarations": 2,
        "prefer-arrow-callback": 2,
        "prefer-spread": 2,
        "arrow-spacing": [2, { "before": true, "after": true }],
        "comma-dangle": 2,
        "no-cond-assign": 2,
        "no-control-regex": 2,
        "no-debugger": 2,
        "no-dupe-keys": 2,
        "no-empty": 2,
        "no-empty-character-class": 2,
        "no-iterator": 2,
        "no-ex-assign": 2,
        "no-extra-boolean-cast": 2,
        "no-extra-parens": 1,
        "no-extra-semi": 2,
        "no-func-assign": 2,
        "no-invalid-regexp": 2,
        "no-irregular-whitespace": 2,
        "no-negated-in-lhs": 2,
        "no-obj-calls": 2,
        "no-regex-spaces": 2,
        "quote-props": [2, "as-needed"],
        "no-sparse-arrays": 2,
        "no-unreachable": 2,
        "use-isnan": 2,
        "valid-typeof": 2,
        "eqeqeq": [2, "smart"],
        "no-alert": 2,
        "no-div-regex": 2,
        "no-else-return": 2,
        "no-empty-label": 2,
        "no-eval": 2,
        "no-extra-bind": 2,
        "no-floating-decimal": 2,
        "no-implied-eval": 2,
        "no-iterator": 2,
        "no-lone-blocks": 2,
        "no-loop-func": 2,
        "no-multi-spaces": 2,
        "no-new-func": 2,
        "no-new-wrappers": 2,
        "no-octal": 2,
        "no-octal-escape": 2,
        "no-redeclare": 2,
        "no-return-assign": 2,
        "no-script-url": 2,
        "no-self-compare": 2,
        "no-sequences": 2,
        "no-throw-literal": 2,
        "no-unused-expressions": 2,
        "no-unneeded-ternary": 2,
        "operator-linebreak": [2, "after"],
        "linebreak-style": 2,
        "no-duplicate-case": 2,
        "no-with": 2,
        "radix": 2,
        "wrap-iife": 2,
        "yoda": [2, "never"],
        "strict": 0,
        "consistent-return": 2,
        "no-bitwise": 2,
        "no-useless-call": 2,
        "no-invalid-this": 2,
        "no-constant-condition": 2,
        "no-dupe-args": 2,
        "no-underscore-dangle": 0,
        "no-unused-vars": 1
    }
}
